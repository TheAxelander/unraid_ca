<?xml version="1.0"?>
<Container version="2">
  <Name>OpenBudgeteer</Name>
  <Repository>axelander/openbudgeteer</Repository>
  <Registry>https://hub.docker.com/r/axelander/openbudgeteer</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/100181-support-axelander-openbudgeteer</Support>
  <Project>https://github.com/TheAxelander/OpenBudgeteer</Project>
  <Overview>OpenBudgeteer is a budgeting app based on the Bucket Budgeting Principle</Overview>
  <Category>Productivity: Tools: Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/TheAxelander/OpenBudgeteer/master/assets/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1607942823</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>OpenBudgeteer is a budgeting app based on the Bucket Budgeting Principle</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6100</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/app/database</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value/>
      <Name>CONNECTION_PROVIDER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_SERVER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_DATABASE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_USER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>CONNECTION_ROOT_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>APPSETTINGS_CULTURE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>APPSETTINGS_THEME</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="WebUI" Target="8080" Default="" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false">6100</Config>
  <Config Name="Database Provider" Target="CONNECTION_PROVIDER" Default="mysql" Mode="" Description="mysql, mariadb, sqlite or postgresql" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Database Server" Target="CONNECTION_SERVER" Default="" Mode="" Description="IP address of database server (not for sqlite)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Database Port" Target="CONNECTION_PORT" Default="" Mode="" Description="Port of database server (not for sqlite)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Database Name" Target="CONNECTION_DATABASE" Default="" Mode="" Description="Name of the database, for SQLite please enter /app/database/ as path and your name of the database (e.g. /app/database/mydb.db)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Database User" Target="CONNECTION_USER" Default="" Mode="" Description="Database user (not for sqlite)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Database Password" Target="CONNECTION_PASSWORD" Default="" Mode="" Description="Password of database user (not for sqlite)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Database Root Password" Target="CONNECTION_ROOT_PASSWORD" Default="" Mode="" Description="Password of database root user to automatically create database and user (mysql, mariadb only)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Sqlite Databse Path" Target="/app/database" Default="" Mode="rw" Description="Path of local sqlite database (sqlite only)" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="Localization" Target="APPSETTINGS_CULTURE" Default="en-US" Mode="" Description="Sets localization like currency and number format (Must be a BCP 47 language tag)" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="Theme" Target="APPSETTINGS_THEME" Default="default" Mode="" Description="Theme that will be used (see https://bootswatch.com/), e.g. darkly, solar etc." Type="Variable" Display="always" Required="false" Mask="false"/>  
</Container>
